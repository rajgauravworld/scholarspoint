<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ProcessLogin</servlet-name>
        <servlet-class>sp.controller.ProcessLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessFeedback</servlet-name>
        <servlet-class>sp.controller.ProcessFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SpecificUserRegistration</servlet-name>
        <servlet-class>sp.controller.SpecificUserRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessRegistration</servlet-name>
        <servlet-class>sp.controller.ProcessRegistration</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProcessLogin</servlet-name>
        <url-pattern>/ProcessLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessFeedback</servlet-name>
        <url-pattern>/ProcessFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SpecificUserRegistration</servlet-name>
        <url-pattern>/SpecificUserRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessRegistration</servlet-name>
        <url-pattern>/ProcessRegistration</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
